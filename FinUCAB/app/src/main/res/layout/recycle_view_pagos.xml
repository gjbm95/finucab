<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_marginTop="5dp"    android:layout_marginLeft="10dp"    android:layout_marginRight="10dp"    android:layout_marginBottom="5dp"    android:background="@drawable/esquinas">    <TextView        android:id="@+id/descripcionpagoTextView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginLeft="10dp"        android:layout_marginTop="10dp"        android:gravity="center_vertical"        android:text="Semestre2017"        android:textColor="@color/black"        android:textSize="20sp"        android:textStyle="bold"/>    <TextView        android:id="@+id/montopagoTextView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:gravity="center_vertical"        android:text="10000bs"        android:textColor="@color/black"        android:textSize="20dp"        android:layout_marginEnd="16dp"        android:layout_centerVertical="true"        android:layout_alignParentEnd="true"        android:layout_marginRight="16dp"        android:layout_alignParentRight="true" />    <TextView        android:id="@+id/categoriapagosTextView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignLeft="@+id/nameTextView"        android:layout_alignStart="@+id/nameTextView"        android:layout_below="@+id/nameTextView"        android:textColor="@color/black"        android:text="Universidad"        android:textSize="14sp"        android:layout_marginEnd="10dp"        android:layout_marginRight="10dp"/></RelativeLayout>