<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    tools:context="com.ucab.fin.finucab.fragment.CuentaFragment">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:text="@string/realizadoExitosamente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:textSize="35sp"
        android:textStyle="bold"
        android:textColor="#fff"
        android:layout_above="@+id/imageView2"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="100dp"
        app:srcCompat="@mipmap/checkicon"
        android:id="@+id/imageView2"
        android:layout_height="200dp"
        android:layout_marginBottom="20dp"
        android:layout_above="@+id/ConfirmarRegistro"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/ConfirmarRegistro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="99dp"
        android:text="Aceptar"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
